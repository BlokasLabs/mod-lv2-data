@prefix fomp:   <http://drobilla.net/plugins/fomp/> .
@prefix mod:    <http://moddevices.com/ns/mod#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .

fomp:autowah
	mod:brand "FOMP" ;
	mod:label "Auto-wah" ;
	modgui:gui [
		modgui:resourcesDirectory <modgui> ;
		modgui:iconTemplate <modgui/icon-auto-wah.html> ;
		modgui:stylesheet <modgui/stylesheet-auto-wah.css> ;
		modgui:screenshot <modgui/screenshot-auto-wah.png> ;
		modgui:thumbnail <modgui/thumbnail-auto-wah.png> ;
		modgui:brand "fomp" ;
		modgui:label "Auto-wah" ;
		modgui:model "boxy" ;
		modgui:panel "5-knobs" ;
		modgui:color "yellow" ;
		modgui:port [
			lv2:index 0 ;
			lv2:symbol "drive" ;
			lv2:name "Drive" ;
		] , [
			lv2:index 1 ;
			lv2:symbol "decay" ;
			lv2:name "Decay" ;
		] , [
			lv2:index 2 ;
			lv2:symbol "range" ;
			lv2:name "Range" ;
		] , [
			lv2:index 3 ;
			lv2:symbol "freq" ;
			lv2:name "Freq" ;
		] , [
			lv2:index 4 ;
			lv2:symbol "mix" ;
			lv2:name "Mix" ;
		] ;
	] .

