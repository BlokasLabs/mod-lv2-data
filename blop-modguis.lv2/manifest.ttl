@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix mod:    <http://moddevices.com/ns/mod#> .
@prefix modgui: <http://moddevices.com/ns/modgui#> .

###############################################################################
## START http://drobilla.net/plugins/blop/sawtooth

<http://drobilla.net/plugins/blop/sawtooth>
	mod:brand "BLOP" ;
	mod:label "Sawtooth" ;
	modgui:gui [
		modgui:resourcesDirectory <modgui> ;
		modgui:iconTemplate <modgui/icon-sawtooth.html> ;
		modgui:stylesheet <modgui/sawtooth.css> ;
		modgui:screenshot <modgui/screenshot-sawtooth.png> ;
		modgui:thumbnail <modgui/thumb-sawtooth.png> ;
	] .

## END http://drobilla.net/plugins/blop/sawtooth
###############################################################################

