@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#panda>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-panda.html> ;
        modgui:stylesheet <modgui/stylesheet-panda.css> ;
        modgui:screenshot <modgui/screenshot-panda.png> ;
        modgui:thumbnail <modgui/thumbnail-panda.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Panda" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "threshold" ;
            lv2:name "Thresh" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "factor" ;
            lv2:name "Ratio" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] ;
    ] .