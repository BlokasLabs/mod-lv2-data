@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#roomy>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-roomy.html> ;
        modgui:stylesheet <modgui/stylesheet-roomy.css> ;
        modgui:screenshot <modgui/screenshot-roomy.png> ;
        modgui:thumbnail <modgui/thumbnail-roomy.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Roomy" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "time" ;
            lv2:name "Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "damping" ;
            lv2:name "Damping" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "dry_wet" ;
            lv2:name "Mix" ;
        ] ;
    ] .