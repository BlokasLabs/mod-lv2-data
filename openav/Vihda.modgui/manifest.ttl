@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#vihda>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-vihda.html> ;
        modgui:stylesheet <modgui/stylesheet-vihda.css> ;
        modgui:screenshot <modgui/screenshot-vihda.png> ;
        modgui:thumbnail <modgui/thumbnail-vihda.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Vihda" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "width" ;
            lv2:name "Width" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "invert" ;
            lv2:name "Invert" ;
        ] ;
    ] .