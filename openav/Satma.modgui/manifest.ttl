@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#satma>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-satma.html> ;
        modgui:stylesheet <modgui/stylesheet-satma.css> ;
        modgui:screenshot <modgui/screenshot-satma.png> ;
        modgui:thumbnail <modgui/thumbnail-satma.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Satma" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "distort" ;
            lv2:name "Drive" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "tone" ;
            lv2:name "Tone" ;
        ] ;
    ] .