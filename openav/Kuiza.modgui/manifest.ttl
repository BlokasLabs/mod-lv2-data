@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#kuiza>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-kuiza.html> ;
        modgui:stylesheet <modgui/stylesheet-kuiza.css> ;
        modgui:screenshot <modgui/screenshot-kuiza.png> ;
        modgui:thumbnail <modgui/thumbnail-kuiza.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Kuiza" ;
        modgui:model "japanese" ;
        modgui:panel "5-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "gain_1" ;
            lv2:name "Gain 1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "gain_2" ;
            lv2:name "Gain 2" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "gain_3" ;
            lv2:name "Gain 3" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "gain_4" ;
            lv2:name "Gain 4" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "gain" ;
            lv2:name "Gain" ;
        ] ;
    ] .