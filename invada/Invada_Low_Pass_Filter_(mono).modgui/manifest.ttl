@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://invadarecords.com/plugins/lv2/filter/lpf/mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-invada-low-pass-filter-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-invada-low-pass-filter-mono.css> ;
        modgui:screenshot <modgui/screenshot-invada-low-pass-filter-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-invada-low-pass-filter-mono.png> ;
        modgui:brand "Invada" ;
        modgui:label "Lowpass Mono" ;
        modgui:model "japanese" ;
        modgui:panel "4-knobs" ;
        modgui:color "white" ;
        modgui:knob "red" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "gain" ;
            lv2:name "Level" ;
        ] ;
    ] .