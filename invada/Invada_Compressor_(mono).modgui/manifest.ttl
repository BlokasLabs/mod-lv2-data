@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://invadarecords.com/plugins/lv2/compressor/mono>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-invada-compressor-mono.html> ;
        modgui:stylesheet <modgui/stylesheet-invada-compressor-mono.css> ;
        modgui:screenshot <modgui/screenshot-invada-compressor-mono.png> ;
        modgui:thumbnail <modgui/thumbnail-invada-compressor-mono.png> ;
        modgui:brand "Invada" ;
        modgui:label "Compressor" ;
        modgui:model "japanese" ;
        modgui:panel "7-a-knobs" ;
        modgui:color "darkblue" ;
        modgui:knob "white" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "threshold" ;
            lv2:name "Thresh" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "ratio" ;
            lv2:name "Ratio" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "rms" ;
            lv2:name "RMS" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "noClip" ;
            lv2:name "Soft Clip" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "gain" ;
            lv2:name "Makeup" ;
        ] ;
    ] .