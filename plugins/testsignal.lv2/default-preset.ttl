@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://gareus.org/oss/lv2/testsignal> ;
	lv2:port [
		lv2:symbol "mode" ;
		pset:value 0
	] , [
		lv2:symbol "reference" ;
		pset:value -18
	] .

