@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://drobilla.net/plugins/blop/adsr> ;
	lv2:port [
		lv2:symbol "attack" ;
		pset:value 0.0
	] , [
		lv2:symbol "decay" ;
		pset:value 0.0
	] , [
		lv2:symbol "release" ;
		pset:value 0.0
	] , [
		lv2:symbol "sustain" ;
		pset:value 1.0
	] .

