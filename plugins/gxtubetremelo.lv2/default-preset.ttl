@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gxtubetremelo#tubetremelo> ;
	lv2:port [
		lv2:symbol "depth" ;
		pset:value 0.0
	] , [
		lv2:symbol "gain" ;
		pset:value 0.0
	] , [
		lv2:symbol "sinewave" ;
		pset:value 0
	] , [
		lv2:symbol "speed" ;
		pset:value 0.1
	] .

