@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://gareus.org/oss/lv2/balance> ;
	lv2:port [
		lv2:symbol "balance" ;
		pset:value 0.0
	] , [
		lv2:symbol "delayLeft" ;
		pset:value 0
	] , [
		lv2:symbol "delayRight" ;
		pset:value 0
	] , [
		lv2:symbol "monoswap" ;
		pset:value 0
	] , [
		lv2:symbol "phaseL" ;
		pset:value 0
	] , [
		lv2:symbol "phaseR" ;
		pset:value 0
	] , [
		lv2:symbol "trim" ;
		pset:value 0.0
	] , [
		lv2:symbol "unitygain" ;
		pset:value 0
	] .

