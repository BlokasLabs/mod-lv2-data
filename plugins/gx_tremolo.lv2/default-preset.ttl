@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gx_tremolo#_tremolo> ;
	lv2:port [
		lv2:symbol "depth" ;
		pset:value 0.5
	] , [
		lv2:symbol "dry_wet" ;
		pset:value 50.0
	] , [
		lv2:symbol "freq" ;
		pset:value 5.0
	] , [
		lv2:symbol "mode" ;
		pset:value 0
	] .

