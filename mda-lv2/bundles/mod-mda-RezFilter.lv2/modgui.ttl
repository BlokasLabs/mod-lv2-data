@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/mda/RezFilter>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-rezfilter.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-rezfilter.css> ;
        modgui:screenshot <modgui/screenshot-mda-rezfilter.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-rezfilter.png> ;
        modgui:brand "MDA" ;
        modgui:label "RezFilter" ;
        modgui:model "boxy" ;
        modgui:panel "8-knobs" ;
        modgui:color "yellow" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "res" ;
            lv2:name "Res" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "output" ;
            lv2:name "Output" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "env_vcf" ;
            lv2:name "Env->VCF" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "attack" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "release" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "lfo_vcf" ;
            lv2:name "LFO->VCF" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "lfo_rate" ;
            lv2:name "LFO Rate" ;
        ] ;
    ] .