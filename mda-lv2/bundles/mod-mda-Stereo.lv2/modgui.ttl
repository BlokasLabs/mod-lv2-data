@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/mda/Stereo>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-stereo.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-stereo.css> ;
        modgui:screenshot <modgui/screenshot-mda-stereo.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-stereo.png> ;
        modgui:brand "MDA" ;
        modgui:label "Stereo" ;
        modgui:model "japanese" ;
        modgui:panel "7-a-knobs" ;
        modgui:color "blue" ;
        modgui:knob "darkblue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "width" ;
            lv2:name "Width" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "delay" ;
            lv2:name "Delay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mod" ;
            lv2:name "Mod" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "rate" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "balance" ;
            lv2:name "Balance" ;
        ] ;
    ] .