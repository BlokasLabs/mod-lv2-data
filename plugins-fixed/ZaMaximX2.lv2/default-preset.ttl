@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <urn:zamaudio:ZaMaximX2> ;
	lv2:port [
		lv2:symbol "ceil" ;
		pset:value -3.0
	] , [
		lv2:symbol "gr" ;
		pset:value 0.0
	] , [
		lv2:symbol "outlevel" ;
		pset:value -45.0
	] , [
		lv2:symbol "rel" ;
		pset:value 25.0
	] , [
		lv2:symbol "thresh" ;
		pset:value 0.0
	] .

