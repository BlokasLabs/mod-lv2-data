@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://calf.sourceforge.net/plugins/EnvelopeFilter> ;
	lv2:port [
		lv2:symbol "attack" ;
		pset:value 20.0
	] , [
		lv2:symbol "bypass" ;
		pset:value 0.0
	] , [
		lv2:symbol "gain" ;
		pset:value 1.0
	] , [
		lv2:symbol "level_in" ;
		pset:value 1.0
	] , [
		lv2:symbol "level_out" ;
		pset:value 1.0
	] , [
		lv2:symbol "lower" ;
		pset:value 80.0
	] , [
		lv2:symbol "mix" ;
		pset:value 0.85000002
	] , [
		lv2:symbol "mode" ;
		pset:value 7
	] , [
		lv2:symbol "release" ;
		pset:value 200.0
	] , [
		lv2:symbol "res" ;
		pset:value 2.0
	] , [
		lv2:symbol "response" ;
		pset:value 0.0
	] , [
		lv2:symbol "sidechain" ;
		pset:value 0.0
	] , [
		lv2:symbol "upper" ;
		pset:value 3000.0
	] .

