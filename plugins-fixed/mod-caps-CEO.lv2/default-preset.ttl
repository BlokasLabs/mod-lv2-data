@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/caps/CEO> ;
	lv2:port [
		lv2:symbol "damping" ;
		pset:value 0.0
	] , [
		lv2:symbol "ppm" ;
		pset:value 80.0
	] , [
		lv2:symbol "volume" ;
		pset:value 0.75
	] .

