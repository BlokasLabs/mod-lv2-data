@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/caps/PhaserII> ;
	lv2:port [
		lv2:symbol "depth" ;
		pset:value 0.75
	] , [
		lv2:symbol "lfo" ;
		pset:value 0
	] , [
		lv2:symbol "rate" ;
		pset:value 0.25
	] , [
		lv2:symbol "resonance" ;
		pset:value 0.25
	] , [
		lv2:symbol "spread" ;
		pset:value 0.75
	] .

