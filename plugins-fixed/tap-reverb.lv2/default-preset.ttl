@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/tap/reverb> ;
	lv2:port [
		lv2:symbol "allps_en" ;
		pset:value 1.0
	] , [
		lv2:symbol "bandpass_en" ;
		pset:value 1.0
	] , [
		lv2:symbol "combs_en" ;
		pset:value 1.0
	] , [
		lv2:symbol "decay" ;
		pset:value 2800.0
	] , [
		lv2:symbol "drylevel" ;
		pset:value -4.0
	] , [
		lv2:symbol "mode" ;
		pset:value 0.0
	] , [
		lv2:symbol "stereo_enh" ;
		pset:value 1.0
	] , [
		lv2:symbol "wetlevel" ;
		pset:value -12.0
	] .

