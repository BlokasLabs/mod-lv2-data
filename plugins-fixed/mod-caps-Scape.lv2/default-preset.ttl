@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/caps/Scape> ;
	lv2:port [
		lv2:symbol "blend" ;
		pset:value 1.0
	] , [
		lv2:symbol "bpm" ;
		pset:value 100.0
	] , [
		lv2:symbol "divider" ;
		pset:value 2.0
	] , [
		lv2:symbol "dry" ;
		pset:value 0.5
	] , [
		lv2:symbol "feedback" ;
		pset:value 0.75
	] , [
		lv2:symbol "tune" ;
		pset:value 440.0
	] .

