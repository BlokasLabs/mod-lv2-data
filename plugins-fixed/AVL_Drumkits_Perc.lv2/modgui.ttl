@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://kxstudio.linuxaudio.org/plugins/FluidPlug_AVL_Drumkits_Perc>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-drumkits-percussion.html> ;
        modgui:stylesheet <modgui/stylesheet-drumkits-percussion.css> ;
        modgui:screenshot <modgui/screenshot-drumkits-percussion.png> ;
        modgui:thumbnail <modgui/thumbnail-drumkits-percussion.png> ;
        modgui:brand "AV Linux" ;
        modgui:label "Drumkits Percussion" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "zinc" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "level" ;
            lv2:name "Level" ;
        ] ;
    ] .