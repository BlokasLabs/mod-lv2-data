@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/caps/ToneStack> ;
	lv2:port [
		lv2:symbol "bass" ;
		pset:value 0.5
	] , [
		lv2:symbol "mid" ;
		pset:value 0.5
	] , [
		lv2:symbol "model" ;
		pset:value 0
	] , [
		lv2:symbol "treble" ;
		pset:value 0.5
	] .

