@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/caps/Saturate> ;
	lv2:port [
		lv2:symbol "bias" ;
		pset:value 0.0
	] , [
		lv2:symbol "gain" ;
		pset:value 0.0
	] , [
		lv2:symbol "mode" ;
		pset:value 1
	] .

