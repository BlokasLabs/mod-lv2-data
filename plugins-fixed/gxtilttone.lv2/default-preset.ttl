@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://guitarix.sourceforge.net/plugins/gxtilttone#tilttone> ;
	lv2:port [
		lv2:symbol "drive" ;
		pset:value 0.0
	] , [
		lv2:symbol "gain" ;
		pset:value 0.0
	] , [
		lv2:symbol "tone" ;
		pset:value 0.5
	] .

