@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <http://moddevices.com/plugins/mod-devel/DS1> ;
	lv2:port [
		lv2:symbol "Dist" ;
		pset:value 0.5
	] , [
		lv2:symbol "Level" ;
		pset:value 0.5
	] , [
		lv2:symbol "Tone" ;
		pset:value 0.5
	] .

