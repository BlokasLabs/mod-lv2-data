@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix pset:  <http://lv2plug.in/ns/ext/presets#> .

<default-preset>
	a pset:Preset ;
	lv2:appliesTo <https://github.com/ninodewit/SHIRO-Plugins/plugins/pitchotto> ;
	lv2:port [
		lv2:symbol "blur" ;
		pset:value 0.25
	] , [
		lv2:symbol "cutoff" ;
		pset:value 2250.0
	] , [
		lv2:symbol "delay1" ;
		pset:value 100.0
	] , [
		lv2:symbol "delay2" ;
		pset:value 100.0
	] , [
		lv2:symbol "mix" ;
		pset:value 0.5
	] , [
		lv2:symbol "ratio1" ;
		pset:value 0.5
	] , [
		lv2:symbol "ratio2" ;
		pset:value 2.0
	] .

