@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/rec_vco>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rec-vco.html> ;
        modgui:stylesheet <modgui/stylesheet-rec-vco.css> ;
        modgui:screenshot <modgui/screenshot-rec-vco.png> ;
        modgui:thumbnail <modgui/thumbnail-rec-vco.png> ;
        modgui:brand "fomp" ;
        modgui:label "Rec VCO" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "green" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "octave" ;
            lv2:name "Octave" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "tune" ;
            lv2:name "Tune" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "waveform" ;
            lv2:name "Wave" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "lp_filrer" ;
            lv2:name "Lowpass" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "exp_fm_gain" ;
            lv2:name "Exp FM" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "lin_fm_gain" ;
            lv2:name "Lin FM" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "form_mod" ;
            lv2:name "Mod" ;
        ] ;
    ] .