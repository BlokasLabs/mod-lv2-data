@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/saw_vco>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-saw-vco.html> ;
        modgui:stylesheet <modgui/stylesheet-saw-vco.css> ;
        modgui:screenshot <modgui/screenshot-saw-vco.png> ;
        modgui:thumbnail <modgui/thumbnail-saw-vco.png> ;
        modgui:brand "fomp" ;
        modgui:label "Saw VCO" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "green" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "octave" ;
            lv2:name "Octave" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "tune" ;
            lv2:name "Tune" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "lp_filter" ;
            lv2:name "Lowpass" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "exp_fm_gain" ;
            lv2:name "Exp FM" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "lin_fm_gain" ;
            lv2:name "Lin FM" ;
        ] ;
    ] .