@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/mvchpf1>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-moog-high-pass-filter-1.html> ;
        modgui:stylesheet <modgui/stylesheet-moog-high-pass-filter-1.css> ;
        modgui:screenshot <modgui/screenshot-moog-high-pass-filter-1.png> ;
        modgui:thumbnail <modgui/thumbnail-moog-high-pass-filter-1.png> ;
        modgui:brand "FOMP" ;
        modgui:label "Moog Highpass" ;
        modgui:model "boxy" ;
        modgui:panel "5-knobs" ;
        modgui:color "black" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "exp_gm_gain" ;
            lv2:name "Exp FM gain" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "in_gain" ;
            lv2:name "Input gain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "out_gain" ;
            lv2:name "Output gain" ;
        ] ;
    ] .