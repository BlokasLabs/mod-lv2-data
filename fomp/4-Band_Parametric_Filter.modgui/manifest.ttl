            lv2:name "Frq4" ;
        ] , [
            lv2:index 10 ;
            lv2:symbol "bw_4" ;
            lv2:name "Bnd4" ;
        ] , [
            lv2:index 11 ;
            lv2:symbol "gain_4" ;
            lv2:name "Gain4" ;
