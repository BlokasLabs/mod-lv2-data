@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/mvclpf2>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-moog-low-pass-filter-2.html> ;
        modgui:stylesheet <modgui/stylesheet-moog-low-pass-filter-2.css> ;
        modgui:screenshot <modgui/screenshot-moog-low-pass-filter-2.png> ;
        modgui:thumbnail <modgui/thumbnail-moog-low-pass-filter-2.png> ;
        modgui:brand "FOMP" ;
        modgui:label "Moog Lowpass 2" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "black" ;
        modgui:knob "aluminium" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "res" ;
            lv2:name "Reson" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "out_gain" ;
            lv2:name "Output" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "exp_fm_gain" ;
            lv2:name "Exp FM" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "res_gain" ;
            lv2:name "Res gain" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "in_gain" ;
            lv2:name "Input" ;
        ] ;
    ] .