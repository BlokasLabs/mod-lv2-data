@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/cs_phaser1_lfo>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cs-phaser-1-with-lfo.html> ;
        modgui:stylesheet <modgui/stylesheet-cs-phaser-1-with-lfo.css> ;
        modgui:screenshot <modgui/screenshot-cs-phaser-1-with-lfo.png> ;
        modgui:thumbnail <modgui/thumbnail-cs-phaser-1-with-lfo.png> ;
        modgui:brand "FOMP" ;
        modgui:label "CS Phaser 1 LFO" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "purple" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "fb_gain" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "sections" ;
            lv2:name "Sections" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "out_mix" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "lfo_freq" ;
            lv2:name "LFO freq" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "lfo_waveform" ;
            lv2:name "LFO wave" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "mod_gain" ;
            lv2:name "Mod" ;
        ] ;
    ] .