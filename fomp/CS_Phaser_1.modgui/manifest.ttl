@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/fomp/cs_phaser1>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-cs-phaser-1.html> ;
        modgui:stylesheet <modgui/stylesheet-cs-phaser-1.css> ;
        modgui:screenshot <modgui/screenshot-cs-phaser-1.png> ;
        modgui:thumbnail <modgui/thumbnail-cs-phaser-1.png> ;
        modgui:brand "fomp" ;
        modgui:label "CS Phaser 1" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "purple" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "fb_gain" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "sections" ;
            lv2:name "Sections" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "out_mix" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "exp_fm_gain" ;
            lv2:name "Exp FM" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "lin_fm_gain" ;
            lv2:name "Lin FM" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "in_gain" ;
            lv2:name "Input" ;
        ] ;
    ] .