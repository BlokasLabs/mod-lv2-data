@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://www.openavproductions.com/artyfx#filta>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-filta.html> ;
        modgui:stylesheet <modgui/stylesheet-filta.css> ;
        modgui:screenshot <modgui/screenshot-filta.png> ;
        modgui:thumbnail <modgui/thumbnail-filta.png> ;
        modgui:brand "OpenAV" ;
        modgui:label "Filta" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "darkblue" ;
        modgui:knob "bronze" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "frequency_control" ;
            lv2:name "Frequency" ;
        ] ;
    ] .