@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mod:   <http://moddevices.com/ns/mod#> .
@prefix rsz:  <http://lv2plug.in/ns/ext/resize-port#> .
@prefix unit: <http://lv2plug.in/ns/extensions/units#> .

<http://distrho.sf.net/plugins/Pitchotto>
    a lv2:Plugin ;
    
    doap:name "Shiro Pitchotto";

    doap:developer [
	foaf:name "Nino de Wit";
	foaf:homepage <http://github.com/ninodewit>;
	foaf:mbox <mailto:ninodig@hotmail.com>;
    ];

    doap:maintainer [
	foaf:name "MOD Team";
	foaf:homepage <http://moddevices.com>;
	foaf:mbox <mailto:devel@moddevices.com>;
    ];
    
    mod:brand "Shiro";
    mod:label "Pitchotto";

    doap:license <http://usefulinc.com/doap/licenses/gpl>;

    lv2:minorVersion 0;
    lv2:microVersion 0;

    rdfs:comment """
    Pitchotto is a pitch-shifter based on the "Pitch-Shift"-genpatch in Max, where Phase-shifting is used to achieve different intervals.
    There are two shifted signals available, both with variable window-sizes for arpeggio-like sounds.
    """;

    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;

    lv2:optionalFeature <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength> ;

    lv2:requiredFeature <http://lv2plug.in/ns/ext/options#options> ,
                        <http://lv2plug.in/ns/ext/urid#map> ;

    lv2:port [
        a lv2:InputPort, lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "audio_in_1" ;
        lv2:name "Audio Input 1" ;
    ] ;

    lv2:port [
        a lv2:OutputPort, lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "audio_out_1" ;
        lv2:name "Audio Output 1" ;
    ] ;

    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:name "mix" ;
        lv2:symbol "mix" ;
        lv2:default 1.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 1.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:name "blur" ;
        lv2:symbol "blur" ;
        lv2:default 0.010000 ;
        lv2:minimum 0.010000 ;
        lv2:maximum 0.250000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:name "ratio1" ;
        lv2:symbol "ratio1" ;
        lv2:default 0.500000 ;
        lv2:minimum 0.500000 ;
        lv2:maximum 2.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:name "ratio2" ;
        lv2:symbol "ratio2" ;
        lv2:default 2.000000 ;
        lv2:minimum 0.500000 ;
        lv2:maximum 2.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:name "window2" ;
        lv2:symbol "window2" ;
        lv2:default 100.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 1000.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:name "cutoff" ;
        lv2:symbol "cutoff" ;
        lv2:default 3000.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 3000.000000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:name "resonance" ;
        lv2:symbol "resonance" ;
        lv2:default 0.000000 ;
        lv2:minimum 0.000000 ;
        lv2:maximum 0.250000 ;
    ] ,
    [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 9 ;
        lv2:name "window1" ;
        lv2:symbol "window1" ;
        lv2:default 100.000000 ;
        lv2:minimum 0.100000 ;
        lv2:maximum 1000.000000 ;
    ] .