@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_redeye#vibrochump>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxredeye-vibro-chump.html> ;
        modgui:stylesheet <modgui/stylesheet-gxredeye-vibro-chump.css> ;
        modgui:screenshot <modgui/screenshot-gxredeye-vibro-chump.png> ;
        modgui:thumbnail <modgui/thumbnail-gxredeye-vibro-chump.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxVibroChump" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "tribal1" ;
        modgui:knob "silver" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Speed" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Intensity" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "Feedback" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "Sinewave" ;
            lv2:name "Sinewave" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "Gain" ;
            lv2:name "Drive" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "Volume" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "Tone" ;
            lv2:name "Tone" ;
        ] ;
    ] .