@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Echoverse>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-echoverse.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-echoverse.css> ;
        modgui:screenshot <modgui/screenshot-rkr-echoverse.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-echoverse.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Echoverse" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "skull" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BYPASS" ;
            lv2:name "Doesnt" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "add" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PAN" ;
            lv2:name "much" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "DELAY" ;
            lv2:name "TEMPO" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LR_DELAY" ;
            lv2:name "Left/Right Delay Offset" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "ANGLE" ;
            lv2:name "Angle" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "FEEDBACK" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "DAMP" ;
            lv2:name "Damping" ;
        ] ;
    ] .