@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#delm>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-musical-delay.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-musical-delay.css> ;
        modgui:screenshot <modgui/screenshot-rkr-musical-delay.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-musical-delay.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Musical Delay" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "red" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEL1" ;
            lv2:name "Subdiv. 1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "FB1" ;
            lv2:name "Feedb. 1" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "DAMP" ;
            lv2:name "Damping" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "DEL2" ;
            lv2:name "Subdiv. 2" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "FB2" ;
            lv2:name "Feedb. 2" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "OFFS" ;
            lv2:name "Offset" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Time" ;
        ] ;
    ] .