@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Shuffle>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-shuffle.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-shuffle.css> ;
        modgui:screenshot <modgui/screenshot-rkr-shuffle.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-shuffle.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Shuffle" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "pink" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "LFREQ" ;
            lv2:name "Freq 1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "M1FREQ" ;
            lv2:name "Freq 2" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "M2FREQ" ;
            lv2:name "Freq 3" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "HFREQ" ;
            lv2:name "Freq 4" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LGAIN" ;
            lv2:name "Gain 1" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "M1GAIN" ;
            lv2:name "Gain 2" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "M2GAIN" ;
            lv2:name "Gain 3" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "HGAIN" ;
            lv2:name "Gain 4" ;
        ] ;
    ] .