@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#ShelfBoost>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-shelf-boost.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-shelf-boost.css> ;
        modgui:screenshot <modgui/screenshot-rkr-shelf-boost.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-shelf-boost.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Shelf Boost" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "LEVEL" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TONE" ;
            lv2:name "Tone" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PRES" ;
            lv2:name "Presence" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "GAIN" ;
            lv2:name "Gain" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "STEREO" ;
            lv2:name "Stereo" ;
        ] ;
    ] .