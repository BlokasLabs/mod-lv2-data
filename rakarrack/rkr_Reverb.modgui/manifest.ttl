@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#reve>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-reverb.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-reverb.css> ;
        modgui:screenshot <modgui/screenshot-rkr-reverb.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-reverb.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "rkr Reverb" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "cyan" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "DAMP" ;
            lv2:name "Damping" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "RS" ;
            lv2:name "Room Size" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "TIME" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LPF" ;
            lv2:name "Lowpass Filter" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "HPF" ;
            lv2:name "Highpass Filter" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "TYPE" ;
            lv2:name "F-verb/rand" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "IDEL" ;
            lv2:name "Pre-delay" ;
        ] ;
    ] .