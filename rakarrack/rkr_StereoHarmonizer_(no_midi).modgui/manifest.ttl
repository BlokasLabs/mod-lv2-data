@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#StereoHarm_no_mid>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-stereoharmonizer-no-midi.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-stereoharmonizer-no-midi.css> ;
        modgui:screenshot <modgui/screenshot-rkr-stereoharmonizer-no-midi.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-stereoharmonizer-no-midi.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "StereoHarmonizer" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "green" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "INTL" ;
            lv2:name "Left Interval" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Wet/Dry" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "GAINL" ;
            lv2:name "Left Gain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "INTR" ;
            lv2:name "Right Interval" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "GAINR" ;
            lv2:name "Right Gain" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "INTR" ;
            lv2:name "Right Interval" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "CHR" ;
            lv2:name "Right Chroma" ;
        ] ;
    ] .