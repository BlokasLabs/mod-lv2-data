@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#DistBand>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-distband.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-distband.css> ;
        modgui:screenshot <modgui/screenshot-rkr-distband.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-distband.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "DistBand" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "LEVEL" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "LMCR" ;
            lv2:name "Low/Mid Crossover" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "MHCR" ;
            lv2:name "Mid/High Crossover" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LVOL" ;
            lv2:name "Low Gain" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "MVOL" ;
            lv2:name "Mid Gain" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "HVOL" ;
            lv2:name "High Gain" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "DRIVE" ;
            lv2:name "Drive" ;
        ] ;
    ] .