@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Echotron>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-echotron.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-echotron.css> ;
        modgui:screenshot <modgui/screenshot-rkr-echotron.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-echotron.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Echotron" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "red" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "FB" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "DAMP" ;
            lv2:name "Dampening" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LFOTYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "WIDTH" ;
            lv2:name "LFO Width" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Filter Depth" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "LENGTH" ;
            lv2:name "Taps" ;
        ] ;
    ] .