@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#eco>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-echo.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-echo.css> ;
        modgui:screenshot <modgui/screenshot-rkr-echo.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-echo.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Echo" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "red" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DELAY" ;
            lv2:name "Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "FEEDBACK" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "DAMP" ;
            lv2:name "Damping" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LR_DELAY" ;
            lv2:name "Offset" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "LRCr" ;
            lv2:name "Crossover" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "REV" ;
            lv2:name "Reverse" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] ;
    ] .