@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#comp>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-compressor.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-compressor.css> ;
        modgui:screenshot <modgui/screenshot-rkr-compressor.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-compressor.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Compressor" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "RATIO" ;
            lv2:name "RATIO" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "THRESHOLD" ;
            lv2:name "Threshold" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "KNEE" ;
            lv2:name "Knee" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "OUTPUT" ;
            lv2:name "Makeup Gain" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "ATTACK" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "RELEASE" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "PEAK" ;
            lv2:name "Peak" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "STEREO" ;
            lv2:name "Stereo" ;
        ] ;
    ] .