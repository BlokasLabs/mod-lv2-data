@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Shifter>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-shifter.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-shifter.css> ;
        modgui:screenshot <modgui/screenshot-rkr-shifter.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-shifter.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Shifter" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "green" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "INT" ;
            lv2:name "Interval" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "MODE" ;
            lv2:name "Mode" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "GAIN" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "ATACK" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "DECAY" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "THRESH" ;
            lv2:name "Threshold" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "DN" ;
            lv2:name "Shift" ;
        ] ;
    ] .