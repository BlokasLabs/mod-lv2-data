@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Dual_Flange>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-dual-flange.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-dual-flange.css> ;
        modgui:screenshot <modgui/screenshot-rkr-dual-flange.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-dual-flange.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "rkr Dual Flange" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "orange" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "FB" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "TYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "LPF" ;
            lv2:name "Lowpass Filter" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "THZ" ;
            lv2:name "ThruZero" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "RND" ;
            lv2:name "Random" ;
        ] ;
    ] .