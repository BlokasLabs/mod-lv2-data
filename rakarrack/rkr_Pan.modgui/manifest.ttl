@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#pan>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-pan.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-pan.css> ;
        modgui:screenshot <modgui/screenshot-rkr-pan.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-pan.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Pan" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "racing" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "AUTO" ;
            lv2:name "Autopan" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "TYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "STDL" ;
            lv2:name "LFO L/R Delay" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "EXTRA_AMOUNT" ;
            lv2:name "Amount" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "EXTRA_ON" ;
            lv2:name "Enable" ;
        ] ;
    ] .