@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#CompBand>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-compband.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-compband.css> ;
        modgui:screenshot <modgui/screenshot-rkr-compband.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-compband.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "CompBand" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "M1R" ;
            lv2:name "Mid1 Ratio" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "M2R" ;
            lv2:name "Mid2 Ratio" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "HR" ;
            lv2:name "Hi Ratio" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "M1T" ;
            lv2:name "Mid1 Thresh" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "M2T" ;
            lv2:name "Mid2 Thresh" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "HT" ;
            lv2:name "Hi Thresh" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "GAIN" ;
            lv2:name "Makeup Gain" ;
        ] ;
    ] .