@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#wha>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-wahwah.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-wahwah.css> ;
        modgui:screenshot <modgui/screenshot-rkr-wahwah.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-wahwah.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "WahWah" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "yellow" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "SENSE" ;
            lv2:name "Sensitivity" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "TYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "MODE" ;
            lv2:name "Filter Type" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "INV" ;
            lv2:name "Invert" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "RND" ;
            lv2:name "Random" ;
        ] ;
    ] .