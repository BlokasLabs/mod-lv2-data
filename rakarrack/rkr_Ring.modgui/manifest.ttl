@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Ring>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-ring.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-ring.css> ;
        modgui:screenshot <modgui/screenshot-rkr-ring.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-ring.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Ring" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "skull" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BYPASS" ;
            lv2:name "not" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "working" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "PAN" ;
            lv2:name "with" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "LRCr" ;
            lv2:name "frequencies" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LEVEL" ;
            lv2:name "?" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "FREQ" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "STEREO" ;
            lv2:name "Stereo" ;
        ] ;
    ] .