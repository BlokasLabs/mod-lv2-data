@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Ring>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-ring.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-ring.css> ;
        modgui:screenshot <modgui/screenshot-rkr-ring.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-ring.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Ring" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "gray" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "FREQ" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "AFREQ" ;
            lv2:name "Auto-Freq" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "SIN" ;
            lv2:name "Sine" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "TRI" ;
            lv2:name "Triangle" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "SAW" ;
            lv2:name "Sawtooth" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "SQUARE" ;
            lv2:name "Square" ;
        ] ;
    ] .