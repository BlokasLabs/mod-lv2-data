@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Synthfilter>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-synthfilter.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-synthfilter.css> ;
        modgui:screenshot <modgui/screenshot-rkr-synthfilter.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-synthfilter.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Synthfilter" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DEPTH" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TEMPO" ;
            lv2:name "Rate" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "FB" ;
            lv2:name "Feedback" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "TYPE" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "LSTAGES" ;
            lv2:name "Lo Stages" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "HSTAGES" ;
            lv2:name "Hi Stages" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "WIDTH" ;
            lv2:name "Width" ;
        ] ;
    ] .