@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Vocoder>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-vocoder.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-vocoder.css> ;
        modgui:screenshot <modgui/screenshot-rkr-vocoder.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-vocoder.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Vocoder" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "blue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "RING" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Q" ;
            lv2:name "Q" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "MUF" ;
            lv2:name "Smear" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WETDRY" ;
            lv2:name "Mix" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LEVEL" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "INGAIN" ;
            lv2:name "Input gain" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "PAN" ;
            lv2:name "Panning" ;
        ] ;
    ] .