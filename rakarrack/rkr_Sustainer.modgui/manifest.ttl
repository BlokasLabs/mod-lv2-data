@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Sustainer>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-sustainer.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-sustainer.css> ;
        modgui:screenshot <modgui/screenshot-rkr-sustainer.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-sustainer.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Sustainer" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "gray" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "SUS" ;
            lv2:name "Sustain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "GAIN" ;
            lv2:name "Gain" ;
        ] ;
    ] .