@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Reverbtron>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-reverbtron.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-reverbtron.css> ;
        modgui:screenshot <modgui/screenshot-rkr-reverbtron.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-reverbtron.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "rkr Reverbtron" ;
        modgui:model "lata" ;
        modgui:panel "8-knobs" ;
        modgui:color "skull" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "BYPASS" ;
            lv2:name "not" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "WETDRY" ;
            lv2:name "fixed" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "FADE" ;
            lv2:name "Fade" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "SAFE" ;
            lv2:name "Safe" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LENGTH" ;
            lv2:name "Length" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "DELAY" ;
            lv2:name "Initial Delay" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "DAMP" ;
            lv2:name "Dampening" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "LEVEL" ;
            lv2:name "Level" ;
        ] ;
    ] .