@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://rakarrack.sourceforge.net/effects.html#Expander>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-rkr-expander.html> ;
        modgui:stylesheet <modgui/stylesheet-rkr-expander.css> ;
        modgui:screenshot <modgui/screenshot-rkr-expander.png> ;
        modgui:thumbnail <modgui/thumbnail-rkr-expander.png> ;
        modgui:brand "Rakarrack" ;
        modgui:label "Expander" ;
        modgui:model "lata" ;
        modgui:panel "7-knobs" ;
        modgui:color "cream" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "THRESHOLD" ;
            lv2:name "Threshold" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "LPF" ;
            lv2:name "Lowpass Filter" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "HPF" ;
            lv2:name "Highpass Filter" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "OUTPUT" ;
            lv2:name "Makeup gain" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "ATTACK" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "RELEASE" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "SHAPE" ;
            lv2:name "Shape" ;
        ] ;
    ] .