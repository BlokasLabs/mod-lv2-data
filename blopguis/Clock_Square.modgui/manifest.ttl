@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/sync_square>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-clock-square.html> ;
        modgui:stylesheet <modgui/stylesheet-clock-square.css> ;
        modgui:screenshot <modgui/screenshot-clock-square.png> ;
        modgui:thumbnail <modgui/thumbnail-clock-square.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Clock Square" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Frequency" ;
        ] ;
    ] .