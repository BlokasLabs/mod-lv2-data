@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/sync_pulse>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-clock-pulse.html> ;
        modgui:stylesheet <modgui/stylesheet-clock-pulse.css> ;
        modgui:screenshot <modgui/screenshot-clock-pulse.png> ;
        modgui:thumbnail <modgui/thumbnail-clock-pulse.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Clock Pulse" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "pwidth" ;
            lv2:name "Pulse Width" ;
        ] ;
    ] .