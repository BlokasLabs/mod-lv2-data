@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/dahdsr>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-retriggerable-dahdsr-envelope.html> ;
        modgui:stylesheet <modgui/stylesheet-retriggerable-dahdsr-envelope.css> ;
        modgui:screenshot <modgui/screenshot-retriggerable-dahdsr-envelope.png> ;
        modgui:thumbnail <modgui/thumbnail-retriggerable-dahdsr-envelope.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Retriggerable" ;
        modgui:model "boxy" ;
        modgui:panel "6-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "delay" ;
            lv2:name "Delay Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "attack" ;
            lv2:name "Attack Time" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "hold" ;
            lv2:name "Hold Time" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "decay" ;
            lv2:name "Decay Time" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "sustain" ;
            lv2:name "Sustain Level" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "release" ;
            lv2:name "Release Time" ;
        ] ;
    ] .