@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/ratio>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-ratio.html> ;
        modgui:stylesheet <modgui/stylesheet-ratio.css> ;
        modgui:screenshot <modgui/screenshot-ratio.png> ;
        modgui:thumbnail <modgui/thumbnail-ratio.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Ratio" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "numerator" ;
            lv2:name "Numerator" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "denominator" ;
            lv2:name "Denominator" ;
        ] ;
    ] .