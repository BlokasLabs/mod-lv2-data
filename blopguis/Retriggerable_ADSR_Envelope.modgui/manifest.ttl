@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/adsr_gt>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-retriggerable-adsr-envelope.html> ;
        modgui:stylesheet <modgui/stylesheet-retriggerable-adsr-envelope.css> ;
        modgui:screenshot <modgui/screenshot-retriggerable-adsr-envelope.png> ;
        modgui:thumbnail <modgui/thumbnail-retriggerable-adsr-envelope.png> ;
        modgui:brand "Blop" ;
        modgui:label "Retriggerable ADSR" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "attack" ;
            lv2:name "Attack Time" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "decay" ;
            lv2:name "Decay Time" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "sustain" ;
            lv2:name "Sustain Level" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "release" ;
            lv2:name "Release Time" ;
        ] ;
    ] .