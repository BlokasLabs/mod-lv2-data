@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/sum>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-sum.html> ;
        modgui:stylesheet <modgui/stylesheet-sum.css> ;
        modgui:screenshot <modgui/screenshot-sum.png> ;
        modgui:thumbnail <modgui/thumbnail-sum.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Sum" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "in1" ;
            lv2:name "Input 1" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "in2" ;
            lv2:name "Input 2" ;
        ] ;
    ] .