@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/sequencer_32>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-32-step-sequencer.html> ;
        modgui:stylesheet <modgui/stylesheet-32-step-sequencer.css> ;
        modgui:screenshot <modgui/screenshot-32-step-sequencer.png> ;
        modgui:thumbnail <modgui/thumbnail-32-step-sequencer.png> ;
        modgui:brand "BLOP" ;
        modgui:label "32 Stepsequencer" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "steps" ;
            lv2:name "Loop Steps" ;
        ] ;
    ] .