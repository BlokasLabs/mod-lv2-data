@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/product>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-product.html> ;
        modgui:stylesheet <modgui/stylesheet-product.css> ;
        modgui:screenshot <modgui/screenshot-product.png> ;
        modgui:thumbnail <modgui/thumbnail-product.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Product" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "multiplicand" ;
            lv2:name "Multiplicand" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "multiplier" ;
            lv2:name "Multiplier" ;
        ] ;
    ] .