@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/lp4pole>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-4-pole-resonant-low-pass.html> ;
        modgui:stylesheet <modgui/stylesheet-4-pole-resonant-low-pass.css> ;
        modgui:screenshot <modgui/screenshot-4-pole-resonant-low-pass.png> ;
        modgui:thumbnail <modgui/thumbnail-4-pole-resonant-low-pass.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Lowpass 4P" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood2" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "cutoff" ;
            lv2:name "Cutoff Frequency" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "resonance" ;
            lv2:name "Resonance" ;
        ] ;
    ] .