@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/fmod>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-frequency-modulator.html> ;
        modgui:stylesheet <modgui/stylesheet-frequency-modulator.css> ;
        modgui:screenshot <modgui/screenshot-frequency-modulator.png> ;
        modgui:thumbnail <modgui/thumbnail-frequency-modulator.png> ;
        modgui:brand "BLOP" ;
        modgui:label "FM" ;
        modgui:model "boxy" ;
        modgui:panel "2-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "freq" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "mod" ;
            lv2:name "MoD Depth" ;
        ] ;
    ] .