@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/interpolator>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-control-to-cv-interpolator.html> ;
        modgui:stylesheet <modgui/stylesheet-control-to-cv-interpolator.css> ;
        modgui:screenshot <modgui/screenshot-control-to-cv-interpolator.png> ;
        modgui:thumbnail <modgui/thumbnail-control-to-cv-interpolator.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Control input to CV" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "in" ;
            lv2:name "Control Input" ;
        ] ;
    ] .