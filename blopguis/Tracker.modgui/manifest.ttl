@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://drobilla.net/plugins/blop/tracker>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-tracker.html> ;
        modgui:stylesheet <modgui/stylesheet-tracker.css> ;
        modgui:screenshot <modgui/screenshot-tracker.png> ;
        modgui:thumbnail <modgui/thumbnail-tracker.png> ;
        modgui:brand "BLOP" ;
        modgui:label "Tracker" ;
        modgui:model "boxy" ;
        modgui:panel "8-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "black" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "hattack" ;
            lv2:name "High Att R" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "hdecay" ;
            lv2:name "High Dec R" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "lattack" ;
            lv2:name "Low Att R" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "ldecay" ;
            lv2:name "Low Dec R" ;
        ] ;
    ] .