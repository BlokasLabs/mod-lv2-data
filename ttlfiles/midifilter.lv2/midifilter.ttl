 , [ a lv2:InputPort, lv2:ControlPort ; lv2:index 10 ; lv2:symbol "randspeed" ; lv2:name "Randomize Acceleration"; lv2:minimum 0.0 ; lv2:maximum 1.0 ; lv2:default 0.0; rdfs:comment "Amount of randomization to apply to the accel/decel setting. A Value of 1.0 means to add a random-number of the full-range (-1..1) to the given value." ]

