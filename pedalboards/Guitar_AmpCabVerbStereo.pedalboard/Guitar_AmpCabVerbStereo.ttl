@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix ingen: <http://drobilla.net/ns/ingen#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi:  <http://lv2plug.in/ns/ext/midi#> .
@prefix pedal: <http://moddevices.com/ns/modpedal#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .

_:b1
    ingen:tail <roomy/out_right> ;
    ingen:head <playback_2> .

_:b2
    ingen:tail <roomy/out_left> ;
    ingen:head <playback_1> .

_:b3
    ingen:tail <CabinetIV/out> ;
    ingen:head <roomy/in_right> .

_:b4
    ingen:tail <CabinetIV/out> ;
    ingen:head <roomy/in_left> .

_:b5
    ingen:tail <capture_1> ;
    ingen:head <AmpVTS/in> .

_:b6
    ingen:tail <AmpVTS/out> ;
    ingen:head <CabinetIV/in> .

<CabinetIV>
    ingen:canvasX 1728.0 ;
    ingen:canvasY 296.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 24 ;
    lv2:minorVersion 24 ;
    lv2:port <CabinetIV/in> ,
             <CabinetIV/out> ,
             <CabinetIV/model> ,
             <CabinetIV/gain> ;
    lv2:prototype <http://moddevices.com/plugins/caps/CabinetIV> ;
    a ingen:Block .

<CabinetIV/in>
    a lv2:AudioPort ,
        lv2:InputPort .

<CabinetIV/in>
    a lv2:AudioPort ,
        lv2:OutputPort .

<CabinetIV/model>
    ingen:value 10.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<CabinetIV/gain>
    ingen:value 9.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS>
    ingen:canvasX 439.0 ;
    ingen:canvasY 376.5 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 24 ;
    lv2:minorVersion 24 ;
    lv2:port <AmpVTS/in> ,
             <AmpVTS/out> ,
             <AmpVTS/over> ,
             <AmpVTS/gain> ,
             <AmpVTS/bright> ,
             <AmpVTS/power> ,
             <AmpVTS/tonestack> ,
             <AmpVTS/bass> ,
             <AmpVTS/mid> ,
             <AmpVTS/treble> ,
             <AmpVTS/attack> ,
             <AmpVTS/squash> ,
             <AmpVTS/low_cut> ;
    lv2:prototype <http://moddevices.com/plugins/caps/AmpVTS> ;
    a ingen:Block .

<AmpVTS/in>
    a lv2:AudioPort ,
        lv2:InputPort .

<AmpVTS/in>
    a lv2:AudioPort ,
        lv2:OutputPort .

<AmpVTS/bass>
    ingen:value 0.250000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/gain>
    ingen:value 0.250000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/low_cut>
    ingen:value 0.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/squash>
    ingen:value 0.250000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/tonestack>
    ingen:value 5.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/treble>
    ingen:value 0.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/power>
    ingen:value 0.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/over>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/attack>
    ingen:value 0.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/mid>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<AmpVTS/bright>
    ingen:value 0.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<roomy>
    ingen:canvasX 2221.3 ;
    ingen:canvasY 296.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 0 ;
    lv2:port <roomy/in_left> ,
             <roomy/in_right> ,
             <roomy/out_left> ,
             <roomy/out_right> ,
             <roomy/time> ,
             <roomy/damping> ,
             <roomy/dry_wet> ;
    lv2:prototype <http://www.openavproductions.com/artyfx#roomy> ;
    a ingen:Block .

<roomy/in_left>
    a lv2:AudioPort ,
        lv2:InputPort .

<roomy/in_right>
    a lv2:AudioPort ,
        lv2:InputPort .

<roomy/in_left>
    a lv2:AudioPort ,
        lv2:OutputPort .

<roomy/in_right>
    a lv2:AudioPort ,
        lv2:OutputPort .

<roomy/time>
    ingen:value 0.250000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<roomy/dry_wet>
    ingen:value 0.250000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<roomy/damping>
    ingen:value 0.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<control_in>
    atom:bufferType atom:Sequence ;
    lv2:index 0 ;
    lv2:name "Control" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "control_in" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:InputPort .

<control_out>
    atom:bufferType atom:Sequence ;
    lv2:index 1 ;
    lv2:name "Control" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "control_out" ;
    <http://lv2plug.in/ns/ext/resize-port#minimumSize> 4096 ;
    a atom:AtomPort ,
        lv2:OutputPort .

<capture_1>
    lv2:index 2 ;
    lv2:name "capture_1" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "capture_1" ;
    a lv2:AudioPort ,
        lv2:InputPort .

<capture_2>
    lv2:index 3 ;
    lv2:name "capture_2" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "capture_2" ;
    a lv2:AudioPort ,
        lv2:InputPort .

<playback_1>
    lv2:index 4 ;
    lv2:name "playback_1" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "playback_1" ;
    a lv2:AudioPort ,
        lv2:OutputPort .

<playback_2>
    lv2:index 5 ;
    lv2:name "playback_2" ;
    lv2:portProperty lv2:connectionOptional ;
    lv2:symbol "playback_2" ;
    a lv2:AudioPort ,
        lv2:OutputPort .

<>
    doap:name "Guitar-AmpCabVerbStereo" ;
    pedal:width 2880 ;
    pedal:height 1444 ;
    pedal:addressings <addressings.json> ;
    pedal:screenshot <screenshot.png> ;
    pedal:thumbnail <thumbnail.png> ;
    ingen:polyphony 1 ;
    ingen:arc _:b1 ,
              _:b2 ,
              _:b3 ,
              _:b4 ,
              _:b5 ,
              _:b6 ;
    ingen:block <CabinetIV> ,
                <AmpVTS> ,
                <roomy> ;
    lv2:port <control_in> ,
             <control_out> ,
             <capture_1> ,
             <capture_2> ,
             <playback_1> ,
             <playback_2> ;
    lv2:extensionData <http://lv2plug.in/ns/ext/state#interface> ;
    a lv2:Plugin ,
        ingen:Graph ,
        pedal:Pedalboard .
